</head>
<body>

    <!-- Navbar -->
<nav class="navbar fixed-top navbar-expand-lg navbar-light bg-white">
  <!-- Container wrapper -->
  <div class="container">    
    <!-- Toggle button -->
    <button
      class="navbar-toggler"
      type="button"
      data-bs-toggle="collapse"
      data-bs-target="#navbarSupportedContent1"
    >
      <i class="fas fa-bars"></i>
    </button>

    <!-- Collapsible wrapper -->
    <div class="collapse navbar-collapse" id="navbarSupportedContent1">      
      <!-- Navbar brand -->
      <a class="navbar-brand mt-2 mt-sm-0" href="#">
        <img
          src="https://talent2win.com/wp-content/uploads/2021/09/LogoDefault.png"
          height="20"
          alt="Logo"
          loading="lazy"
        />
      </a>
      <!-- Left links -->
      <ul class="navbar-nav me-auto mb-2 mb-lg-0">
        <li class="nav-item active">
          <a class="nav-link " href="{{path('home')}}">Home</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="{{path('product_show')}}">Product</a>
        </li>
      </ul>
      <!-- Left links -->      
    </div>
    <!-- Collapsible wrapper -->

    
    
    <!-- Right elements -->
    <div class="d-flex align-items-center">
      
       {% if app.user %}
          
           <a class="nav-link me-3" href="#">
            <i class="fas fa-shopping-cart"></i>
          </a>
           Welcome {{ app.user.name }},
          <a class="nav-link me-3" href="{{ path('app_logout') }}">Logout</a>
        {% else %}
        <a class="nav-link" href="{{ path('app_login') }}"><i class="fas fa-sign-in-alt"></i></a>
        
          <a class="nav-link" href="{{ path('app_register') }}">Register</a> 
        
        {% endif %}
 


      <!-- Icon -->
       <a class="nav-link me-3" href="#">
        <i class="fas fa-shopping-cart"></i>
      </a>
      <a class="nav-link me-3" href="#">
        <i class="fas fa-sign-in-alt"></i>
      </a>
    </div>
    <!-- Right elements -->
    
  </div>
  <!-- Container wrapper -->
</nav>
<!-- Navbar -->
   <link href="https://use.fontawesome.com/releases/v5.7.2/css/all.css" rel="stylesheet">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <div style="height: 60px"></div>

{% block body %}
 <!--Main layout-->
<main>
<div class="container">
  <!-- Navbar -->
<nav class="navbar navbar-expand-lg navbar-dark mt-3 mb-5 shadow p-2" style="background-color: blue">
  <!-- Container wrapper -->
  <div class="container-fluid">

    <!-- Navbar brand -->
    <a class="navbar-brand" href="#">Categories:</a>

    <!-- Toggle button -->
    <button 
       class="navbar-toggler" 
       type="button" 
       data-bs-toggle="collapse" 
       data-bs-target="#navbarSupportedContent2">
      <i class="fas fa-bars"></i>
    </button>

    <!-- Collapsible wrapper -->
    <div class="collapse navbar-collapse" id="navbarSupportedContent2">
      <ul class="navbar-nav me-auto mb-2 mb-lg-0">

        <!-- Link -->
        <li class="nav-item acitve">
          <a class="nav-link text-white" href="#">All</a>
        </li>

      </ul>

      <!-- Search -->
      <form class="w-auto py-1" style="max-width: 12rem">
        <input type="search" class="form-control rounded-0" placeholder="Search" aria-label="Search">
      </form>

    </div>
  </div>
  <!-- Container wrapper -->
</nav>
<!-- Navbar -->


 <div class="maincontent-area">
        <div class="zigzag-center"></div>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="latest-product">
                        <h2 class="section-title">Lish of Product </h2>
                        <div class="product-carousel">
                       </div>
                    </div>
                </div>
            </div>
        </div>
    </div> 
  
  
<!-- Products -->
  <section>
  <div class="text-center">
    <div class="row">
      {% for p in products %}
        <div class="col-lg-3 col-md-6 mb-4">
        <div class="card">
          <div class="bg-image hover-zoom ripple ripple-surface ripple-surface-light"
            data-mdb-ripple-color="light">
            {% set img = p.image %}
            <img src="{{asset('uploads/'~img)}}"
              class="w-100" />
          </div>
          <div class="card-body">
            <a href="{{path('product_read',{"id":p.id})}}" class="text-reset">
              <h5 class="card-title mb-2">{{p.name}}</h5>
            </a>
            <h6 class="mb-3 price">{{p.price}}$</h6>
          </div>
        </div>
      </div>
      {% endfor %}
    </div>

   
  </div>
</section>
</div>
</main>
 <!--Main layout-->

 

{% endblock %}

